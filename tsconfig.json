{
	"include": ["src", "types"],
	"compilerOptions": {
		"plugins": [{ "name": "typescript-plugin-css-modules" }],
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "node",
		"jsx": "preserve",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"baseUrl": "./",
		"paths": {},
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true
	}
}
